<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="org.springframework.entity.Person" name="person">
		<property name="name" value="zhangsan"></property>
		<property name="age" value="11"></property>
	</bean>
	<bean class="org.springframework.selfbdrpp.MyBeanDefinitionRegistryPostProcessor"/>
	<bean class="org.springframework.selfbdrpp.MyBeanFactoryPostProcessor"/>
</beans>